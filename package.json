{
  "name": "salemghoweri.com",
  "version": "0.0.0-development",
  "description": "Salem Ghoweri's Personal Site + Blog",
  "main": "index.js",
  "scripts": {
    "test": "eslint *.js src/**/*.mjs",
    "build": "eleventy & rollup -c",
    "glitch": "git pull -s recursive -X theirs origin master && refresh",
    "prestart": "if [ ! -d \"./public\" ]; then npm run build; fi",
    "_watch": "trap 'kill %1 && exit 0' SIGINT; node server.js & rollup -cw",
    "start": "trap 'kill %1 && exit 0' SIGINT; rollup -cw & yarn serve",
    "serve": "eleventy --watch --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sghoweri/salemghoweri.com.git"
  },
  "keywords": [
    "Blog"
  ],
  "config": {
    "publicDir": "_site/build"
  },
  "author": "Salem Ghoweri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sghoweri/salemghoweri.com/issues"
  },
  "homepage": "https://github.com/sghoweri/salemghoweri.com#readme",
  "dependencies": {
    "sass-mq": "^5.0.1",
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "luxon": "^1.22.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "rollup-plugin-scss": "^2.1.0",
    "autoprefixer": "^9.7.4",
    "node-sass": "^4.13.1",
    "npm-sass": "^2.3.0",
    "postcss-scss": "^2.0.0",
    "rollup-plugin-alias": "^2.2.0",
    "preact": "^10.3.4",
    "preact-html": "^2.0.3",
    "rollup-plugin-yaml": "^2.0.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-livereload": "^1.0.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "core-js": "^3.6.4",
    "date-fns": "^2.11.0",
    "dynamic-import-polyfill": "^0.1.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "highlight.js": "^9.18.1",
    "lodash-es": "^4.17.15",
    "moment": "^2.24.0",
    "nunjucks": "^3.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "regenerator-runtime": "^0.13.5",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.4.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rxjs": "^6.5.4",
    "ua-parser-js": "^0.7.21"
  }
}
